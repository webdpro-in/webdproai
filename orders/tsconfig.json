{
   "compilerOptions": {
      "target": "ES2022",
      "module": "commonjs",
      "lib": [
         "es2022",
         "dom"
      ],
      "strict": true,
      "preserveConstEnums": true,
      "outDir": "dist",
      "sourceMap": true,
      "esModuleInterop": true,
      "types": [
         "node"
      ]
   },
   "include": [
      "src/**/*"
   ],
   "exclude": [
      "node_modules"
   ]
}